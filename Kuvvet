import java.util.Scanner;

public class Main {
    public static void main(String[] args) {

        int n;

        Scanner input=new Scanner(System.in);
        System.out.printf("Bir sayı giriniz:");
        n= input.nextInt();

        System.out.println(n +" sayısına kadar olan 3'ün kuvvatleri:");
        for (int a=1;a<=n;a*=3){
            System.out.println(a);
        }


        System.out.println(n +" sayısına kadar olan 4'ün kuvvatleri:");
        for (int b=1;b<=n;b *= 4){
            System.out.println(b);
        }
    }
}
